<div class="container my-5 search-form">
  <div class="title">
    <h4>Search for tickets</h4>
  </div>
  <div class="hz">
    <%= simple_form_for :search, url: event_tickets_path(@event), method: "GET", html: { class: 'form-inline' } do |f| %>
        <%= f.input :last_name, input_html: {value: ""}, label: '' %>
        <%= f.submit "Search", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
